{
	"name": "spreadsheetcolab.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "prisma generate && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"git:push": "vitest run && commit-and-tag-version && git push --follow-tags origin main",
		"start": "node build"
	},
	"devDependencies": {
		"@bobthered/tailwindcss-palette-generator": "^4.0.0",
		"@prisma/client": "^5.21.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.7",
		"@sveltejs/adapter-vercel": "^5.4.5",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"bcryptjs": "^2.4.3",
		"canvas-confetti": "^1.9.3",
		"commit-and-tag-version": "^12.5.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"prisma": "^5.21.1",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^4.0.0",
		"sveltewind": "^3.0.0",
		"tailwind-merge": "^2.5.4",
		"tailwindcss": "^3.4.9",
		"tw-to-css": "^0.0.12",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"resend": "^4.0.0"
	}
}
